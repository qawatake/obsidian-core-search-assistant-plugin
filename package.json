{
	"name": "obsidian-sample-plugin",
	"version": "0.12.0",
	"description": "This is a sample plugin for Obsidian (https://obsidian.md)",
	"main": "main.js",
	"scripts": {
		"fix:prettier": "prettier --write src",
		"lint:prettier": "prettier --check src",
		"fix": "pnpm fix:prettier && pnpm fix:eslint",
		"fix:eslint": "eslint src --ext .ts --fix",
		"lint": "pnpm lint:prettier && pnpm lint:eslint",
		"lint:eslint": "eslint src --ext .ts,.svelte",
		"dev": "node esbuild.config.mjs",
		"sass-build": "sass --no-source-map *.scss styles.css",
		"esbuild": "node esbuild.config.mjs production",
		"build": "pnpm esbuild && pnpm sass-build"
	},
	"keywords": [],
	"author": "",
	"license": "MIT",
	"devDependencies": {
		"@tsconfig/svelte": "^3.0.0",
		"@types/node": "^17.0.5",
		"@typescript-eslint/eslint-plugin": "^5.8.1",
		"@typescript-eslint/parser": "^5.8.1",
		"builtin-modules": "^3.2.0",
		"esbuild": "~0.13.12",
		"esbuild-svelte": "^0.6.2",
		"eslint": "^8.5.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-react": "^7.28.0",
		"eslint-plugin-svelte3": "^3.4.0",
		"npm-run-all": "^4.1.5",
		"obsidian": "^0.13.11",
		"prettier": "^2.5.1",
		"sass": "^1.48.0",
		"svelte": "^3.46.4",
		"svelte-preprocess": "^4.10.3",
		"tslib": "^2.3.1",
		"typescript": "^4.5.4"
	}
}
